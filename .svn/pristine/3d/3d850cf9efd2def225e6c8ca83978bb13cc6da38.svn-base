/*右上角图片事件*/
.mapType-wrapper{
   z-index: 8888;
    position: absolute;
    right: 0;
    top:0;
}

.mapType {
    cursor: pointer;
}
.expend{
    background-color:rgba(255,255,255,0.8);
}
.mapTypeBg {
    height: 100px;
    cursor: pointer;
    -webkit-transition-property: width,background-color;
    transition-property: width,background-color;
    -webkit-transition-duration: .4s;
    transition-duration: .4s;
     background-color: rgba(255,255,255,.5);
    opacity: 1;
    position: absolute;
    right: 0;
    z-index: 20;
    
}
.mapTypeUl {
    width:100%;
    height:100px;
    background:rgba(255,255,255,.5);
}
.mapTypeUl li{
    float:left;
    margin-right: 5px;
}

.mapType .normal {
    z-index: 21;
    right: 20px;
    background: url('../images/public/map/earth.png') no-repeat;
    background-size:80px 80px;
}
.mapType .earth {
    z-index: 22;
    right: 10px;
    background:url('../images/public/map/yuanqu.png') no-repeat;
    background-size:80px 80px;
}

.mapType .panorama {
    z-index: 23;
    right: 10px;
    background: url('../images/public/map/yuanqu.png') no-repeat;
    border-left: 1px solid rgba(153,153,153,.6);
    background-size:80px;
    height: 80px;
    width: 80px;
    position: absolute;
    border-radius: 2px;
    top: 10px;
    right:10px;
    box-sizing: border-box;
    box-shadow:0 0 3px #ccc;
    border: 1px solid transparent;
    border: 1px solid rgba(153,153,153,.42);
    -webkit-transition-property: right,background-image;
    transition-property: right,background-image;
    -webkit-transition-duration: .4s;
    transition-duration: .4s;
    background:#fff;
    background-repeat: no-repeat;
    background-size:80px 80px;
}
.mapType .mapTypeCard {
    height: 80px;
    width: 80px;
    position: absolute;
    border-radius: 2px;
    top: 10px;
    right:10px;
    box-sizing: border-box;
    box-shadow:0 0 3px #ccc;
    border: 1px solid transparent;
    border: 1px solid rgba(153,153,153,.42);
    -webkit-transition-property: right,background-image;
    transition-property: right,background-image;
    -webkit-transition-duration: .4s;
    transition-duration: .4s;
     background-size:80px 80px;
}

.mapType .switch-box {
    visibility: hidden;
    position: absolute;
    width: 100%;
    height: 20px;
    line-height: 22px;
    color: #fff;
    background: rgba(0,0,0,.5);
}
.mapType .mapTypeCard span,.mapTypeCard2 span,.panorama span {
    position: absolute;
    bottom:0;
    right: 0;
    display: inline-block;
    font-size: 12px;
    color: #FFF;
    border-top-left-radius: 2px;
    height:20px;
    line-height:20px;
    width:100%;
    text-align:center;
    background-color:rgba(0,0,0,0.2);
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    padding:0 5px;
}


.mapactive{
    border:1px solid #079de0;
}
.mapType .mapTypeCard.active span,.mapType .mapTypeCard:hover span,.mapTypeCard2:hover span,.panorama span :hover{
    background-color:#3385FF;

}
.mapType .mapTypeCard:hover,.mapTypeCard2:hover,.panorama:hover{
    border:1px solid #3385FF

}
.mapType .normal:hover .switch-box{
    visibility:visible;
}
.mapType .satellite:hover .switch-box,.mapType .earth:hover .switch-box{
    visibility:visible;
}

