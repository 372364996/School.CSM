/*气泡*/
.bubble {
    width: 111px;
    height: 95px;
    /*position: absolute;*/
    z-index: 888;
    opacity: 1;
    overflow: hidden;
    background: url(../../../style/base/images/public/map/bubbling.png) no-repeat;
    -webkit-animation: bubbles ease-in 3s alternate;
    -moz-animation: bubbles ease-in 3s alternate;
    -o-animation: bubbles ease-in 3s alternate;
    animation: bubbles ease-in 3s alternate;
    background-size: 110px 70px;
    text-align: left;
    padding-top: 25px;
    padding-left: 4px;
    padding-bottom: 32px;
    padding-right: 5px;
    color: #fff;
}

.bubble p {
    font-size: 12px;
    color: #fff;
    width: 102px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    margin: 0;
}

