        /// <summary>
        /// 级别名称
        /// </summary>
        public string level_name { get; set; }
        /// <summary>
        /// 级别状态
        /// </summary>
        public int level_status { get; set; }
        /// <summary>
        /// 颜色
        /// </summary>
        public string level_color { get; set; }
        /// <summary>
        /// 楼宇id
        /// </summary>
        public int building_id { get; set; }
        /// <summary>
        /// 建筑名称
        /// </summary>
        public string building_name { get; set; }
        /// <summary>
        /// 别名
        /// </summary>
        public string alias { get; set; }
        /// <summary>
        /// 负责人
        /// </summary>
        public int manager_id { get; set; }
        /// <summary>
        /// 建筑年代
        /// </summary>
        public string create_time { get; set; }
        /// <summary>
        /// 结构类型
        /// </summary>
        public string building_type { get; set; }
        /// <summary>
        /// 地上面积
        /// </summary>
        public string above_ground_area { get; set; }
        /// <summary>
        /// 地下面积
        /// </summary>
        public string under_ground_area { get; set; }
        /// <summary>
        /// 楼层数
        /// </summary>
        public int floor_num { get; set; }
        /// <summary>
        /// 楼宇类型
        /// </summary>
        public int area_type_id { get; set; }
        /// <summary>
        /// 行业类型
        /// </summary>
        public int industry_id { get; set; }
        /// <summary>
        /// 人员名称（对应人员编号）
        /// </summary>
        public string person_name { get; set; }
        /// <summary>
        /// 管理员名称（对应负责人id）
        /// </summary>
        public string manager_name { get; set; }

    }
}
