using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSM.Model.CustomModel
{
    public class AreaInfoCustom
    {
        /// <summary>
		/// id
        /// </summary>		
        public int id { set; get; }
        /// <summary>
        /// 区域名称
        /// </summary>		
        public string area_name { set; get; }
        /// <summary>
        ///区域坐标
        /// </summary>		
        public string area_location { set; get; }
        /// <summary>
        /// 2.5D区域坐标
        /// </summary>
        public string area_25D_location { get; set; }
        /// <summary>
        /// 区域级别
        /// </summary>		
        public int area_level { set; get; }
        /// <summary>
        /// 区域类别
        /// -1:楼
        /// 0：逻辑区
        /// >0:自己编辑的功能区
        /// </summary>		
        public int area_type { set; get; }
        /// <summary>
        /// 人员编号
        /// </summary>		
        public int person_id { set; get; }
        /// <summary>
        /// 地图类型
        /// 2D：1
        /// 2.5D：2
        /// </summary>		
        public int map_type { set; get; }
        /// <summary>
        /// 在地图是否显示，true：显示，false：不显示
        /// </summary>		
        public bool is_show { set; get; }
        /// <summary>
        /// 备注
        /// </summary>		
        public string content { set; get; }
        /// <summary>
        /// 网格ID
        /// </summary>		
        public int grid_id { set; get; }
        /// <summary>
        /// 园区编号
