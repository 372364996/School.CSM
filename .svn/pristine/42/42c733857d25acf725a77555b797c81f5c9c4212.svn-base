/*筛选条件*/

.screen-title {
    width: 65px;
    height: 20px;
    background: #2aabd2;
    color: #fff;
    text-align: center;
    line-height: 20px;
    margin-top: -8px;
    margin-left: 8px;
}
.search-form {
    width: 220px;
    overflow: hidden;
    border: 1px solid #ccc;
    float: left;
    margin-right: 15px;
    margin-top: 3px;
    height: 29px;
    line-height: 27px;
}

.search-form .search-bd {
    height: 25px;
}

.search-form .search-hd {
    height: 30px;
    background-color: #fff;
    position: relative;
    border: 0;
}

.search-div {
    width: auto;
    height: 29px;
    padding-left: 6px;
    font-size: 12px;
}

.findBtn {
    position: absolute;
    left: 6px;
    top: 1px;
    width: auto;
    height: 0;
    padding-left: 0;
}

.search-form .search-hd .search-input {
    color: #666;
    width: 142px;
    height: 28px !important;
    line-height: 25px;
    /*text-indent: 5px;*/
    border: 0;
    outline: none;
    position: absolute;
    top: -1px;
    z-index: 5;
    font-size: 12px;
    right: -1px;
    padding:0 5px;
}
.input-148width{
    width:148px !important;
}
.input-185width{
    width:185px !important;
}
.search-form .search-hd .pholder {
    display: inline-block;
    padding: 2px 0;
    font-size: 12px;
    color: #999;
    position: absolute;
    left: 13px;
    top: 11px;
    z-index: 4;
    background: url(images/zoom.jpg) no-repeat 0 0;
    padding-left: 25px;
    top: 11px;
}

/*右侧内容*/
.plan-current {
    margin-left: 160px;
}

.plan-current .content-left {
    width: 50%;
    height: 100%;
    min-height: 765px;
    background: none !important;
    position: absolute;
    left: 175px !important;
}

