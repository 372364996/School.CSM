<!--右边部分 start-->
<div class="tab  right-tab">
    <p class="switch-p">大屏配置</p>
</div>
<div class="equip-list List minWidth">
    <p class="screen-title">筛选条件</p>
    <div class="search-form">
        <div id="search-hd" class="search-hd">
            <div class="search-div">园区</div>
            <select class="search-input input-185width" onchange="largeScreen.changeRegion()" id="regionSelect">
                @*<option></option>*@
                @for (int i = 0; i < regionList.Count; i++)
                {
                    <option value=@regionList[i].id>@regionList[i].region_name</option>
                }
            </select>

        </div>
    </div>
    <div class="search-form">
        <div id="search-hd" class="search-hd">
            <div class="search-div">行数</div>
            <input type="number" placeholder="输入行数,<10" class="search-input input-185width" id="rowInput" onchange="largeScreen.changeRowOrColumn()"/>

        </div>
    </div>
    <div class="search-form">
        <div id="search-hd" class="search-hd">
            <div class="search-div">列数</div>
            <input type="number" placeholder="输入列数,<20" class="search-input input-185width" id="columnInput" onchange="largeScreen.changeRowOrColumn()"/>
        </div>
    </div>
    <div class="search-form">
        <div id="search-hd" class="search-hd">
            <div class="search-div">分辨率宽</div>
            <input type="number" placeholder="输入分辨率宽,<3072" class="search-input" id="widthInput"/>
        </div>
    </div>
    <div class="search-form">
        <div id="search-hd" class="search-hd">
            <div class="search-div">分辨率高</div>
            <input type="number" placeholder="输入分辨率高,<2048" class="search-input" id="heightInput"/>
        </div>
    </div>
    <div class="search-btn searchBtn">
        <button type="submit" class="btn btn-primary query" onclick="largeScreen.saveBtn()">保存</button>
    </div>
</div>
