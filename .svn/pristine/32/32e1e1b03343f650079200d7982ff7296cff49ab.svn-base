.registerMotion{
    width: 25px;
    height: 70px;
    background: #2aabd2;
    color:#fff !important;
    text-align: center;
    line-height: 20px;
    position: absolute;
    right: 0;
    margin-right: 0%;
    top: 30%;
    writing-mode: lr-tb;
    padding-top: 15px;
    cursor:pointer;
    transition: margin-right 1s ease;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    display:none;
}
.registerMotion2{
    width: 25px;
    height: 70px;
    background: #2aabd2;
    color:#fff !important;
    text-align: center;
    line-height: 20px;
    position: absolute;
    right: 0;
    margin-right:35.5%;
    top: 30%;
    writing-mode: lr-tb;
    padding-top: 15px;
    transition: margin-right 1s ease;
    cursor:pointer;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}
