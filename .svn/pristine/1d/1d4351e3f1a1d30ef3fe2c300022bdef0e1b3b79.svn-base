/*左侧slider背景，字体颜色*/
.slider,.slider1,.slider2 {background: -moz-linear-gradient(top, #1e465c 0%, #20333df 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1e465c), color-stop(100%,#20333d));
    background: -webkit-linear-gradient(top, #1e465c 0%,#20333d 100%);
    background: -o-linear-gradient(top, #1e465c 0%,#20333d 100%);
    background: -ms-linear-gradient(top, #1e465c 0%,#20333d 100%);
    background: linear-gradient(to bottom, #1e465c 0%,#20333d 100%);}
.submenu li a {color:#fff;}
.submenu li a:hover {color:#fff !important;}
