.garden span{color:#fff !important;}
.submenu li:hover{background:#1281DB;cursor:pointer;color:#fff;}
.slider-word{border-bottom: 1px solid #1f262e;border-top: 1px solid #627282;}
.slider-word:hover { background:#0B8DE8;line-height: 45px;}
.menu-title h3 {/*color:#acb4bf;*/color:#fff;}
.menu-title:hover h3{color:#fff;cursor:pointer;}
.slider-menu .menu-title:hover {background: #1d8ad0;color:#fff;}
