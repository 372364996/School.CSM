
@{
    ViewBag.Title = "接处警列表";
    Layout = "~/Views/Shared/_AAC_Layout.cshtml";
    List<CSM.Model.BaseRegionConfigModel> masterRegionList = ViewData["MasterRegion"] as List<CSM.Model.BaseRegionConfigModel>;
}
@section HeadCSS{
    <link href="~/scripts/js/plugins/easyui/jquery-easyui-1.5/themes/gray/easyui.css" rel="stylesheet" />
    <link href="~/scripts/js/plugins/easyui/jquery-easyui-1.5/themes/icon.css" rel="stylesheet" />
    <link href="~/style/base/css/popup.css" rel="stylesheet" />
    <link href="~/style/base/css/src/bootstrap-multiselect.css" rel="stylesheet" />
}
@section HeadJS{
    <script src="~/scripts/js/plugins/easyui/jquery-easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/scripts/js/plugins/My97DatePicker/My97DatePicker/WdatePicker.js"></script>
    <!--文件上传插件-->
    <script src="~/scripts/js/fileUpload/webuploader.js"></script>
    <!--下拉搜索框-->
    <script src="~/scripts/js/frames/bootstrap-multiselect.js"></script>
    <!--业务-->
    <script src="~/scripts/js/aac/aacCommon.js"></script>
    <script src="~/scripts/js/common/publicMethod.js"></script>
    @*<script src="~/scripts/js/aac/reportType.js"></script>*@
    <script src="~/scripts/js/aac/addAac.js"></script>
    <script src="~/scripts/js/aac/aacDetails.js"></script>
    <script src="~/scripts/js/aac/aacIndex.js"></script>
    <!--滚动条JS-->
    <script src="~/style/base/js/src/jquery.mousewheel.min.js"></script>
    <script src="~/style/base/js/src/jquery.mCustomScrollbar.min.js"></script>
}
<script type="text/javascript">
    //调用webSocket子页面的方法
    function wsChild(msg) {
    }

    var reporttypeList = @Html.Raw(ViewData["ReportTypeList"].ToString());//默认视频平台
</script>

<style>
       .btn-default{
        margin-top: 0px;
        height: 28px;
        line-height:16px;
    }
  .styles .btn-default{
      border:0 !important;
  }
    .styles .btn-group{
        width:158px!important;
        float: right!important;
    }
    .rules{
        float:left;
        margin-right:15px;
        margin-top: 2px;
    }
    .rules .styles{
        border:1px solid #ccc;
        height:30px;
        padding-left:5px;
        width:220px;
        font-size:12px;
    }
     .styles .down-menu{
        width:158px!important;
        clear:both;
    }
       .register-left form table{
        margin-top: 0 !important;
         margin-left: 0 !important;
    }
        .aacDetils-frame table input[type=checkbox]{
            margin-left:8px !important;
        }
</style>
<div class="row report">
    <!--页面内容 start-->
    <div class="slider-content plan-content">
        <div class="plan-list aacList maxWidth ">
            <p class="screen-title">筛选条件</p>
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">园区</div>
                    <select class="search-input" id="regionName">
                        <option value="-1">全部</option>
                        @for (int i = 0; i < masterRegionList.Count; i++)
                        {
                            <option value=@masterRegionList[i].id>@masterRegionList[i].region_name</option>
                        }
                    </select>
                </div>
            </div>
            <div class="rules">
                <label class="styles">案件类型<span class="loaded"><select class="form-control" id="searchreport_type" ></select></span></label>
            </div>
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">案件编号</div>
                    <input type="text" class="search-input" id="searchreport_num" placeholder="">
                </div>
            </div>
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">案件状态</div>
                        <select class="search-input" id="searchincident_status">
                            <option value="0">无</option>
                            <option value="1">接警</option>
                            <option value="2">处警</option>
                            <option value="3">结案</option>
                        </select>
                </div>
            </div>
   
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">报案人姓名</div>
                    <input type="text" class="search-input input-148width" id="searchreport_person_name" placeholder="">
                </div>
            </div>
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">联系电话</div>
                    <input type="text" class="search-input" id="searchreport_person_phone" placeholder="">
                </div>
            </div>
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">开始时间</div>
                    <input type="text" placeholder="" id="searchstartTime" class="Wdate search-input" onfocus="WdatePicker({ isShowClear: true, skin: 'whyGreen', maxDate: '#F{$dp.$D(\'searchstartTime\')}', firstDayOfWeek: 7, startDate: '%y-%M-00 00:00:00', dateFmt: 'yyyy-MM-dd HH:mm:ss', alwaysUseStartDate: true, readOnly: true, maxDate: '#F{$dp.$D(\'searchendTime\')}' })">

                </div>
            </div>
            <div class="search-form">
                <div id="search-hd" class="search-hd">
                    <div class="search-div">结束时间</div>
                    <input type="text" placeholder="" id="searchendTime" class="Wdate search-input" onfocus="WdatePicker({ isShowClear: true, skin: 'whyGreen', minDate: '#F{$dp.$D(\'searchendTime\')}', firstDayOfWeek: 7, startDate: '%y-%M-00 00:00:00', dateFmt: 'yyyy-MM-dd HH:mm:ss', alwaysUseStartDate: true, readOnly: true, minDate: '#F{$dp.$D(\'searchstartTime\')}' })">

                </div>
            </div>
            <div class="search-btn searchBtn">
                <div class="div-empty" onclick="clearSearch()" style="margin-left:0;">清空</div>
                <button type="button" class="btn btn-primary query" onclick="searchReportInfo(1,30)">查询</button>
            </div>

            </div>
        <!--右边存放数据列表的DIV-->
        <div class="tab-content" style="width:500px !important;">
            <table class="aacheight maxWidth" id="cameraScheduleTable"></table>
        </div>


        <!--预案列表中存放列表的DIV-->
        <div class="plan-listbox planList aacListbox">
            <div class="plan-listdiv">
                <table id="report_dg" class="maxWidth aacheight" rownumbers="true" toolbar="#searchtool"
                       loadmsg="正在查询."></table>
            </div>
        </div>


    </div>
</div>

<!--触警弹框-->
<div id="handle_dialog" class="easyui-dialog aac-frame" title="处警" width="800" height="500" style="display:none;">
    <div class="about-close"></div>
    <div class="aac-content">
        <form>
            <table align="center" class="aacTab">
                <tr  id="handle_type_tr" style="display:none">
                    @*<td>报警编号：</td>
                    <td width="270"><input id="report_id" type="text" readonly=true class="form-control " placeholder="" style="width:220px !important"></td>*@
                    <td>处警类型：</td>
                    <td>
                        <select id="handle_type" style="display:" class="form-control">
                            <option value="1">处警</option>
                            <option value="2">结案</option>
                        </select>
                    </td>
                 </tr>
                <tr>
                    <td width="80">处警人：</td>
                    <td  width="280">
                        <select id="handle_person" class="form-control">
                            <option></option>
                        </select>
                    </td>
                    <td width="80">处警时间：</td>
                    <td>
                        <input type="text" placeholder="" id="handle_time" class="Wdate3 form-control" onfocus="WdatePicker({ isShowClear: true, skin: 'whyGreen', maxDate: '#F{$dp.$D(\'handle_time\')}', firstDayOfWeek: 7, startDate: '%y-%M-00 00:00:00', dateFmt: 'yyyy-MM-dd HH:mm:ss', alwaysUseStartDate: true, readOnly: true })">
                    </td>
                </tr>
                <tr style="height:73px">
                    <td class="vAlignTop">处警内容：</td>
                    <td colspan="3">
                        <textarea id="handle_text" class="form-control" style="height:45px!important;width:558px !important"></textarea>
                    </td>
                </tr>
                <tr style="height:237px;">
                    <td class="vAlignTop" style="position:absolute;margin-top:-15px;">关联文件：</td>
                    <td colspan="3">
                        <div class="ordinance-tab2" style="margin-top:-20px">
                            <table id="handlefile_dg" style="width:558px; height:210px !important;overflow-y:auto;"></table>
                        </div>
                    </td>
                </tr>
            </table>
        </form>
        <div class="TabFootBody">
            <div class="tabFoot">
                <input id="handle_button" type="button" name="name" value="提交" class="btn btn-default query" onclick="addHandelInfo()">
                <input type="button" name="name" value="取消" class="btn btn-default queryD" onclick="$('#handle_dialog').dialog('close')">
            </div>
        </div>
    </div>
</div>

