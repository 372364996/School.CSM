                <label id="alarmId"></label>

            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">确警人：</label>
            <div class="col-sm-8">
                @*<input class="form-control" id="confirmPerson" disabled="disabled">*@
                <label id="confirmPerson"></label>

            </div>
        </div>
        <div class="form-group groupStyle">
            <label class="col-sm-4 control-label">确警结论：</label>
            <div class="col-sm-8">
                <select class="form-control formStyle" id="confirmResult">
                    @foreach (CSM.Model.CustomModel.EnumModel item in confirmAlarmResultList)
                    {
                        <option id="@item.key">@item.value</option>
                    }
                </select>

            </div>
        </div>
        <div class="form-group groupStyle">
            <label class="col-sm-4 control-label">确警位置：</label>
            <div class="col-sm-8">
                <input class="form-control formStyle" id="confirmLocation">
            </div>
        </div>
        <div class="form-group groupStyle">
            <label class="col-sm-4 control-label">确警描述：</label>
            <div class="col-sm-8">
                @*<input class="form-control" id="confirmContent">*@
                <textarea class="form-control formStyle" rows="3" id="confirmContent"></textarea>
            </div>
        </div>
    </form>
