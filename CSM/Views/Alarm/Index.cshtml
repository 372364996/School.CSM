<script type="text/javascript">


    //综合告警页面
    $(function () {
        var largeHeight = $(window).height();
        var largeWidth = $(window).width();
        if (largeWidth <= 1400) {
            $(".menu-open").click(function () {
                $(".statistics-box").css({ "width": "80.5px", "height": "20px", "line-heigth": "20px" });
                $(".devicecreate-right2").css("width", "40%");
            })
            $(".menu-close").click(function () {
                $(".statistics-box").css({ "width": "92px", "height": "20px", "line-heigth": "20px" });
            })
        } else if (largeWidth > 1401 && largeWidth <= 1499) {
            $(".menu-open").click(function () {
                $(".statistics-box").css({ "width": "88px", "height": "25px", "line-heigth": "25px" });

            })
            $(".menu-close").click(function () {
                $(".statistics-box").css({ "width": "100px", "height": "25px", "line-heigth": "25px" });

            })
        } else if (largeWidth > 1450 && largeWidth <= 1680) {
            $(".menu-open").click(function () {
                $(".statistics-box").css({ "width": "106px", "height": "25px", "line-heigth": "25px" });
            })
            $(".menu-close").click(function () {
                $(".statistics-box").css({ "width": "118px", "height": "25px", "line-heigth": "25px" });
            })
        }


    })
    $(function () {

        //综合告警页面滚动条
        $(".alarm-table").mCustomScrollbar({
            scrollButtons: {
                enable: false,
                scrollType: "continuous",
                scrollSpeed: 20,
                scrollAmount: 40
            },
            horizontalScroll: false,
        });
    })
    //调用webSocket子页面的方法
    function wsChild(msg) {
        // deviceAlarm.loadDeviceAlarmList(pageIndex, pageSize);
        deviceAlarm.GetDeviceAlarmList(pageIndex, pageSize);
        deviceAlarm.GetAlarmCount();
    }




</script>
